<h1 style="font-weight: 800">Conclusion Overview</h1>

<div style="
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
    flex-direction: column;
  ">
  <!-- Table Head  -->
  <div style="
      display: flex;
      align-items: center;
      justify-content: center;
      width: 100%;
      border-top-left-radius: 10px;
      border-top-right-radius: 10px;
      overflow: hidden;
    ">
    <div class="conclusion-table-head" style="width: 350px">
      <h1>Overview</h1>
    </div>
    <div class="conclusion-table-head" style="width: 450px">
      <h1>Notes</h1>
    </div>
    <div class="conclusion-table-head" style="width: 250px">
      <h1>Result</h1>
    </div>
  </div>
  <!-- Table Body -->
  <div style="
      position: relative;
      display: flex;
      align-items: center;
      justify-content: center;
      width: 100%;
      flex-direction: column;
      border-bottom-left-radius: 17px;
      border-bottom-right-radius: 17px;
      overflow: hidden;
    ">
    <div style="position: absolute; top: -27px; left: 0; width: 100%; height: 100%">
      <svg xmlns="http://www.w3.org/2000/svg" width="124" height="144" viewBox="0 0 94 124" fill="none">
        <path
          d="M-24.1288 23.6976L-24.1274 23.6993L13.08 70.9202L13.0804 70.9208C14.1731 72.3037 16.2742 72.3036 17.367 70.9208L17.3674 70.9202L54.5707 23.7028C54.5712 23.7021 54.5717 23.7015 54.5722 23.7009C54.9064 23.2855 55.392 23.0407 55.9174 23.0319L90.9822 22.4899C90.9823 22.4899 90.9824 22.4899 90.9824 22.4899C92.4093 22.4724 93.2494 24.0722 92.4355 25.2339L16.6483 133.026C15.9533 134.013 14.504 134.013 13.8108 133.026L13.8107 133.026L-61.9778 25.2341C-62.7923 24.07 -61.9476 22.4723 -60.5362 22.4899L-25.4601 23.0319C-24.9415 23.0407 -24.4488 23.2878 -24.1288 23.6976Z"
          stroke="#C1C1C1" />
        <path
          d="M41.8169 22.486L41.8169 22.4861L16.9433 55.3106L16.9431 55.3108C16.0816 56.449 14.3708 56.4494 13.5077 55.3106L-11.3657 22.4863C-12.0907 21.5285 -11.8955 20.1615 -10.9306 19.4454L13.9412 0.997758C14.7039 0.432992 15.7472 0.432989 16.5099 0.997758L41.383 19.4454C42.3478 20.1616 42.5429 21.5283 41.8169 22.486Z"
          stroke="#C1C1C1" />
      </svg>
    </div>
    <!-- Table Rows  -->
    <!-- Row Start  -->
    <div style="
        display: flex;
        align-items: center;
        justify-content: center;
        width: 100%;
      ">
      <div class="conclusion-table-body" style="width: 350px">
        <h1>Honeypot</h1>
      </div>
      <div class="conclusion-table-body" style="width: 450px">
        <h1>The contract owner can drain the funds from contract</h1>
      </div>
      <div class="conclusion-table-body" style="width: 250px">
        <h1>
          <%= summary.table.is_honeypot==="0" ? "✅ False" : "❌ True" %>
        </h1>
      </div>
    </div>
    <!-- Row Start  -->
    <div style="
        display: flex;
        align-items: center;
        justify-content: center;
        width: 100%;
      ">
      <div class="conclusion-table-body" style="width: 350px">
        <h1>Anti whale check</h1>
      </div>
      <div class="conclusion-table-body" style="width: 450px">
        <h1>Features preventing whales to manipulate the Token</h1>
      </div>
      <div class="conclusion-table-body" style="width: 250px">
        <h1>
          <%= summary.table.is_antiwhale=="1" ? "✅ True" : "❌ False" %>
        </h1>
      </div>
    </div>
    <!-- Row Start  -->
    <div style="
        display: flex;
        align-items: center;
        justify-content: center;
        width: 100%;
      ">
      <div class="conclusion-table-body" style="width: 350px">
        <h1>Opensource</h1>
      </div>
      <div class="conclusion-table-body" style="width: 450px">
        <h1>The code of the contract is public</h1>
      </div>
      <div class="conclusion-table-body" style="width: 250px">
        <h1>
          <%= summary.table.is_open_source==="1" ? "✅ True" : "❌ False" %>
        </h1>
      </div>
    </div>
    <!-- Row Start  -->
    <div style="
        display: flex;
        align-items: center;
        justify-content: center;
        width: 100%;
      ">
      <div class="conclusion-table-body" style="width: 350px">
        <h1>Ownership renounced</h1>
      </div>
      <div class="conclusion-table-body" style="width: 450px">
        <h1>Contract owner has renouncedownership</h1>
      </div>
      <div class="conclusion-table-body" style="width: 250px">
        <h1>
          <%= summary.table.owner_address=="" ? "✅ True" : "❌ False" %>
        </h1>
      </div>
    </div>
    <!-- Row Start  -->
    <div style="
        display: flex;
        align-items: center;
        justify-content: center;
        width: 100%;
      ">
      <div class="conclusion-table-body" style="width: 350px">
        <h1>Buy tax</h1>
      </div>
      <div class="conclusion-table-body" style="width: 450px">
        <h1>Fees incurred when buying the token</h1>
      </div>
      <div class="conclusion-table-body" style="width: 250px">
        <h1>
          <%= Number(summary.table.buy_tax)> 5 ? "❌ High buy tax" : "✅" %>
        </h1>
      </div>
    </div>
    <div style="
        display: flex;
        align-items: center;
        justify-content: center;
        width: 100%;
      ">
      <div class="conclusion-table-body" style="width: 350px">
        <h1>Sell tax</h1>
      </div>
      <div class="conclusion-table-body" style="width: 450px">
        <h1>Fees incurred when selling the token</h1>
      </div>
      <div class="conclusion-table-body" style="width: 250px">
        <h1>
          <%= Number(summary.table.sell_tax)> 5 ? "❌ High sell tax" : "✅" %>
        </h1>
        %
      </div>
    </div>
    <div style="
        display: flex;
        align-items: center;
        justify-content: center;
        width: 100%;
      ">
      <div class="conclusion-table-body" style="width: 350px">
        <h1>High Severity Issues</h1>
      </div>
      <div class="conclusion-table-body" style="width: 450px">
        <h1>Number of High severity issues</h1>
      </div>
      <div class="conclusion-table-body" style="width: 250px">
        <h1>
          <%= summary.table.number_of_high_severity_issues %>
        </h1>
      </div>
    </div>
    <div style="
        display: flex;
        align-items: center;
        justify-content: center;
        width: 100%;
      ">
      <div class="conclusion-table-body" style="width: 350px">
        <h1>Medium Severity Issues</h1>
      </div>
      <div class="conclusion-table-body" style="width: 450px">
        <h1>Number of Medium severity issues</h1>
      </div>
      <div class="conclusion-table-body" style="width: 250px">
        <h1>
          <%= summary.table.number_of_medium_severity_issues %>
        </h1>
      </div>
    </div>
    <div style="
        display: flex;
        align-items: center;
        justify-content: center;
        width: 100%;
      ">
      <div class="conclusion-table-body" style="width: 350px">
        <h1>Mintable</h1>
      </div>
      <div class="conclusion-table-body" style="width: 450px">
        <h1>Can mint new tokens</h1>
      </div>
      <div class="conclusion-table-body" style="width: 250px">
        <h1>
          <%= summary.table.is_mintable=="0" ? "✅ False" : "❌ True" %>
        </h1>
      </div>
    </div>
    <div style="
        display: flex;
        align-items: center;
        justify-content: center;
        width: 100%;
      ">
      <div class="conclusion-table-body" style="width: 350px">
        <h1>Blacklist</h1>
      </div>
      <div class="conclusion-table-body" style="width: 450px">
        <h1>Owner can blacklist users</h1>
      </div>
      <div class="conclusion-table-body" style="width: 250px">
        <h1>
          <%= security.is_blacklisted=="0" ? "✅ False" : "❌ True" %>
        </h1>
      </div>
    </div>
    <div style="
        display: flex;
        align-items: center;
        justify-content: center;
        width: 100%;
      ">
      <% let holders=summary.table.holder_count%>
        <div class="conclusion-table-body" style="width: 350px">
          <h1>Holders</h1>
        </div>
        <div class="conclusion-table-body" style="width: 450px">
          <h1>Total wallets holding the token</h1>
        </div>
        <div class="conclusion-table-body" style="width: 250px">
          <h1>
            <%= Number(summary.table.holder_count)> 10000 ? `✅ ${holders}` : `
              ${holders}` %>
          </h1>
        </div>
    </div>
    <div style="
        display: flex;
        align-items: center;
        justify-content: center;
        width: 100%;
      ">
      <% let lpHolder=summary.table.lp_holder_count %>
        <div class="conclusion-table-body" style="width: 350px">
          <h1>LP holder</h1>
        </div>
        <div class="conclusion-table-body" style="width: 450px">
          <h1>Total wallets holding the token</h1>
        </div>
        <div class="conclusion-table-body" style="width: 250px">
          <h1>
            <%= Number(summary.table.lp_holder_count)> 3 ? `✅ ${lpHolder}` : `
              ${lpHolder}` %>
          </h1>
        </div>
    </div>
  </div>
</div>